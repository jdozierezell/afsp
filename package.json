	"scripts": {
		"bs": "gatsby build && gatsby serve",
		"build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build",
		"develop": "set NODE_ENV=development&&gatsby develop",
		"lighthouse": "npm run lighthousea && npm run lighthouses",
		"lighthousea": "cd lighthouse && lighthouse https://afsp.org --view true --output html",
		"lighthouses": "cd lighthouse && lighthouse https://staging--reverent-payne-b0d24a.netlify.app/ --view true --output html",
		"lighthouse9": "cd lighthouse && lighthouse http:localhost:9000 --view true --output html",
		"noIndex": "npm run build && echo 'User-agent: *\nDisallow: /' > dist/robots.txt",
		"format": "prettier --write src/**/*.{js,jsx}",
		"profile": "gatsby build -- --profile",
		"secure": "set NODE_ENV=development&&gatsby develop -- --https",
		"serve": "set NODE_ENV=production&&gatsby serve",
		"stage": "set NODE_ENV=development&&gatsby build",
		"start": "npm run develop",
		"test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
	},
